<!-- 11.1 twig -->
<html>
<head>
  {% block title none|include_title %}
  {{none|include_http_metas}}
  {{none|include_metas}}
  <link href="templates/11.1/css/css.css" rel="stylesheet" type="text/css">
  <link href="templates/base/css/base.css" rel="stylesheet" type="text/css">
</head>

<body background="templates/11.1/images/fdo01.jpg" leftmargin="0" topmargin="0" marginwidth="0" marginheight="0">
<table width="780" height="100%" border="0" align="center" cellpadding="0" cellspacing="0" class="text">
  <tr>
    <td width="10" height="90" background="templates/11.1/images/bor_left.jpg">&nbsp;</td>
    <td width="200" height="90" align="center" valign="middle" background="templates/11.1/images/fdo_logo.jpg">
      <table width="180" border="0" cellpadding="0" cellspacing="0" class="text_big">
        <tr>
          <td><strong>{{website.firmName|break}}</strong></td>
        </tr>
      </table>
    </td>
    <td width="560" height="90"><table width="560" height="90" border="0" cellpadding="0" cellspacing="0">
        <tr>
          <td height="60" background="templates/11.1/images/fdo_top.jpg">&nbsp;</td>
        </tr>
        <tr>
          <td bgcolor="#000000"><table width="560" border="0" cellpadding="0" cellspacing="0" class="text">
              <tr>
                <td width="10">&nbsp;</td>
                <td width="538"><div align="right"><font color="#EBDBB7">{{website.firmName}}</font></div></td>
                <td width="12">&nbsp;</td>
              </tr>
            </table></td>
        </tr>
      </table></td>
    <td width="10" height="90" background="templates/11.1/images/bor_right.jpg">&nbsp;</td>
  </tr>
  <tr>
    <td width="10" height="100%" background="templates/11.1/images/bor_left.jpg">&nbsp;</td>
    <td width="200" height="100%" align="center" valign="top" background="templates/11.1/images/fdo04.jpg">
      <table width="190" border="0" cellpadding="0" cellspacing="0" class="text">
        <tr height="5">
          <td height="5"></td>
        </tr>
        {% for loop_page in website.mainMenu %}
        {# This is the highlighted menu tab #}
        {% if loop_page.url == page.url %}
        <tr>
          <td height="40" background="templates/11.1/images/fdo_bt3.jpg"><table width="190" border="0" cellpadding="0" cellspacing="0" class="text">
              <tr>
                <td width="24">&nbsp;</td>
                <td width="166"><b><a class="menu_text" href="{{loop_page.url}}">{{loop_page.linkName}}</a></b></td>
              </tr>
            </table></td>
        </tr>
        {% else %}
        {# These are the regular menu tabs #}
        <tr>
          <td height="40" background="templates/11.1/images/fdo_bt.jpg"><table width="190" border="0" cellpadding="0" cellspacing="0" class="text">
              <tr>
                <td width="24">&nbsp;</td>
                <td width="166"><b><a class="menu_text" href="{{loop_page.url}}">{{loop_page.linkName}}</a></b></td>
              </tr>
            </table></td>
        </tr>
        {% endif %}
        {% endfor %}
        <tr>
          <td height="40">&nbsp;</td>
        </tr>
        <tr>
          <td height="40" align="center" valign="top">
<table width="180" border="0" cellpadding="0" cellspacing="0" class="text">
              <tr>
                <td><span class="text_fr02">CONTACT US</span><br> <br>
                  <span class="text_fr"><font color="#FFFFFF">
                      {{website.firmName|break}}</font></span><br><br>
                  <font color="#FFCC99">
                    {{website.address}}<br />
                    {{website.city}}, {{website.state}} {{website.zip}}<br />
                    Phone: {{website.phone|format_phone}}<br />
                    Fax: {{website.fax|format_phone}}
                  </font>
                  <br />
                  <a class="link_text" href="mailto:{{website.email}}">{{website.email}}</a>
                  {% if website.socialMedia.avvo  %}
                    <div style="margin:5px 0px;">
                      <a href="{{website.socialMedia.avvo}}" id="avvo_badge" title="View my profile on Avvo"><img src="templates/base/images/avvo_badge.jpg" alt="View my profile on Avvo" title="View my profile on Avvo" /></a>
                    </div>
                  {% endif %}
                </td>
              </tr>
            </table>
          </td>
        </tr>
        <tr>
          <td height="40">&nbsp;</td>
        </tr>
      </table>
    </td>
    <td width="560" height="100%" valign="top" background="templates/11.1/images/fdo02.jpg"><table width="560" height="100%" border="0" cellpadding="0" cellspacing="0" class="text">
        <tr>
          <td height="170"><img src="templates/11.1/images/img_central.jpg" width="560" height="170"></td>
        </tr>
        <tr>
          <td height="50" align="center" background="templates/11.1/images/fdo_fr.jpg"> <table width="530" border="0" cellspacing="0" cellpadding="0">
              <tr>
                <td class="text_big"><strong>{{page.title}}</strong></td>
              </tr>
            </table></td>
        </tr>
        <tr>
          <td height="100%" align="center" valign="top" background="templates/11.1/images/fdo_text01.jpg">
            <table width="500" border="0" cellpadding="0" cellspacing="0" class="text">
              <tr>
                <td>&nbsp;</td>
              </tr>
              <tr>
                <td>
                  {{page.body}}
              </tr>
              <tr>
                <td>&nbsp;</td>
              </tr>
            </table></td>
        </tr>

        <tr>
          <td height="100%" align="center" valign="top" background="templates/11.1/images/fdo_text02.jpg"></td>
        </tr>
        <tr>
          <td height="38" align="center" background="templates/11.1/images/fdo_bottom.jpg"> <table width="530" border="0" cellpadding="0" cellspacing="0" class="text">
              <tr>
                <td width="326" class="text"><font color="#FFCC99"><a class="link_text" href="http://esqsites123.com">Legal web design</a> made easy by Esqsites123.com</font></td>
                <td width="204" class="text"><div align="right">
				{% for loop_page in website.footerMenu %}
                  <a href="{{loop_page.url}}" class="link_text">{{loop_page.linkName}}</a>
                  {% if not loop.last %}
                  <font color="white"> | </font>
                  {% endif %}
                {% endfor %}
				</div></td>
              </tr>
            </table></td>
        </tr>
      </table></td>
    <td width="10" height="100%" background="templates/11.1/images/bor_right.jpg">&nbsp;</td>
  </tr>
</table>
</body>
</html>