<!-- 7.1 twig -->
<html>
<head>
  {% block title none|include_title %}
  {{none|include_http_metas}}
  {{none|include_metas}}
  <link href="templates/7.1/css/css.css" rel="stylesheet" type="text/css">
  <link href="templates/base/css/base.css" rel="stylesheet" type="text/css">
</head>

<body bgcolor="#EEEEEE" leftmargin="0" topmargin="0" marginwidth="0" marginheight="0">
<table width="750" border="0" align="center" cellpadding="0" cellspacing="0" bgcolor="#FFFFFF">
  <tr>
    <td width="24" background="templates/7.1/images/left_back.jpg">&nbsp;</td>
    <td colspan="3"><table width="100%" height="212"  border="0" cellpadding="0" cellspacing="0">
      <tr>
        <td width="234" background="templates/7.1/images/top_bg_1.jpg"><table width="100%"  border="0" cellspacing="0" cellpadding="0">
          <tr>
            <td width="13%">&nbsp;</td>
            <td width="78%" class="lawOfficesOf_white">{{website.firmName|break}}</td>
            <td width="9%">&nbsp;</td>
          </tr>
        </table></td>
        <td width="188" background="templates/7.1/images/main2.jpg">&nbsp;</td>
        <td width="182" background="templates/7.1/images/main3.jpg">&nbsp;</td>
        <td width="146" background="templates/7.1/images/main4.jpg">&nbsp;</td>
      </tr>
    </table></td>
    <td width="24" background="templates/7.1/images/right_back.jpg">&nbsp;</td>
  </tr>
  <tr>
    <td width="24" rowspan="6" background="templates/7.1/images/left_back.jpg"><img src="templates/7.1/images/left_back.jpg" width="24" height="6"></td>
    <td colspan="3"> <TABLE WIDTH=747 BORDER=0 CELLPADDING=0 CELLSPACING=0>
          <TR>
          <td colspan="3" background="templates/7.1/images/menu_bg.jpg">
<table height="36" width="" border="0" cellpadding="8" cellspacing="0" >
	<tr>
	{% for loop_page in website.mainMenu %}
    {# This is the highlighted menu tab #}
	{% if loop_page.url == page.url %}
      <td background="templates/7.1/images/menu_bg_rollover.jpg" align="center" valign="bottom" height="35">
      <strong><a class="menutext_on" href="{{loop_page.url}}">{{loop_page.linkName}}</a></strong>		</td>
    {% else %}
	{# These are the regular menu tabs #}
      <td  align="center" valign="bottom" height="35">
      <strong><a class="menutext" href="{{loop_page.url}}">{{loop_page.linkName}}</a></strong>		</td>
    {% endif %}
    {% endfor %}
	</tr>
</table></td>
        </TR>
      </TABLE></td>
    <td width="24" rowspan="6" background="templates/7.1/images/right_back.jpg"><img src="templates/7.1/images/right_back.jpg" width="24" height="6"></td>
  </tr>
  <tr>
    <td width="195" valign="top">&nbsp;</td>
    <td width="27">&nbsp;</td>
    <td width="525">&nbsp;</td>
  </tr>
  <tr>
    <td rowspan="3" align="center" valign="top" bgcolor="#38507E" class="address"><table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0">
      <tr>
        <td><table width="100%" height="50" border="0" cellpadding="0" cellspacing="0">
      <tr>
        <td align="center" valign="middle">&nbsp;</td>
      </tr>
      <tr>
        <td align="center" valign="middle" class="address">
          {{website.phone|format_phone}}
        </td>
      </tr>
      <tr>
        <td>&nbsp;</td>
      </tr>
      <tr>
        <td>&nbsp;</td>
      </tr>
    </table></td>
      </tr>
      <tr>
        <td>&nbsp;</td>
      </tr>
    </table></td>
    <td height="50">&nbsp;</td>
    <td rowspan="2" valign="top"> <table width="520" border="0" cellspacing="0" cellpadding="0">
      <tr>
        <td height="51" colspan="2" valign="middle" class="lawOfficesOf">{{page.title}}</td>
          <td width="2%">&nbsp;</td>
        </tr>
      <tr>
        <td colspan="2"><span>
          {{page.body}}
          </span></td>
          <td>&nbsp;</td>
        </tr>
      <tr>
        <td width="94%"  class="innertext2">&nbsp;</td>
          <td width="4%"  class="innertext2">&nbsp;</td>
          <td>&nbsp;</td>
        </tr>
    </table></td>
  </tr>
  <tr>
    <td>&nbsp;</td>
  </tr>
  <tr>
    <td height="119" bgcolor="#B8CEFF">&nbsp;</td>
    <td valign="top" bgcolor="#B8CEFF">
<table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
          <td height="10" colspan="6">&nbsp;</td>
        </tr>
        <tr>
          <td width="304" valign="top">&nbsp;</td>
          <td width="15" valign="top"><img src="templates/7.1/images/clear.gif" width="4" height="1"></td>
          <td width="4" valign="top" bgcolor="#9FB3D5">&nbsp;</td>
          <td width="12" valign="top"><img src="templates/7.1/images/clear.gif" width="4" height="1"></td>
          <td width="181" valign="top" class="lefttext">
            <span class="bodytext">Contact</span><span class="innertext2"><span class="innertext2a"><br>
            <br>
            </span>
              Phone: {{website.phone|format_phone}}<br />
              Fax: {{website.fax|format_phone}}<br />
              Email: {{website.email}}</span>
            {% if website.socialMedia.avvo  %}
              <div style="margin:5px 0px;">
                <a href="{{website.socialMedia.avvo}}" id="avvo_badge" title="View my profile on Avvo"><img src="templates/base/images/avvo_badge.jpg" alt="View my profile on Avvo" title="View my profile on Avvo" /></a>
              </div>
            {% endif %}
          </td>
          <td width="8"><img src="templates/7.1/images/clear.gif" width="4" height="1"></td>
        </tr>
        <tr>
          <td valign="top">&nbsp;</td>
          <td valign="top">&nbsp;</td>
          <td valign="top" bgcolor="#9FB3D5">&nbsp;</td>
          <td valign="top">&nbsp;</td>
          <td valign="top" class="lefttext">&nbsp;</td>
          <td>&nbsp;</td>
        </tr>
      </table>    </td>
  </tr>
  <tr>
    <td height="40" colspan="3" align="right" class="t01"><table width="100%"  border="0" cellspacing="0" cellpadding="0">
      <tr>
        <td height="10" colspan="4"></td>
        </tr>
      <tr>
        <td width="4%">&nbsp;</td>
        <td width="64%" class="innertext2">
          {{website.address}}<br />
          {{website.city}}  {{website.state}} {{website.zip}}</td>
        <td width="29%"><div align="right">
          {% for loop_page in website.footerMenu %}
			<a href="{{loop_page.url}}" class="menutext">{{loop_page.linkName}}</a>
            {% if not loop.last %}
            <font color="black"> &nbsp;|&nbsp; </font>
            {% endif %}
          {% endfor %}
		</div></td>
        <td width="3%">&nbsp;</td>
      </tr>
	  <tr>
        <td width="4%">&nbsp;</td>
        <td colspan="2" class="innertext2"><div align="left">Design provided by EsqSites123.com - <a href="http://www.esqsites123.com/" class="innertext2">Legal Web Design</a> </div></td>
        <td width="3%">&nbsp;</td>
      </tr>
      <tr>
        <td height="10" colspan="4"></td>
        </tr>
    </table></td>
  </tr>
</table>
</body>
</html>