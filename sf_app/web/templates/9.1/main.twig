<!-- 9.1 twig -->
<html>
<head>
  {% block title none|include_title %}
  {{none|include_http_metas}}
  {{none|include_metas}}
  <link href="templates/9.1/css/css.css" rel="stylesheet" type="text/css">
  <link href="templates/base/css/base.css" rel="stylesheet" type="text/css">
</head>

<body bgcolor="#535353" leftmargin="0" topmargin="0" marginwidth="0" marginheight="0">
<table width="778" border="0" align="center" cellpadding="0" cellspacing="0" bgcolor="#FFFFFF">
  <tr>
    <td colspan="2" align="left" valign="top" background="templates/9.1/images/top_03.jpg"><IMG SRC="templates/9.1/images/top_01.jpg" WIDTH=32 HEIGHT=28></td>
    <td colspan="2" valign="top" background="templates/9.1/images/top_03.jpg">&nbsp;</td>
    <td colspan="2" align="right" valign="top" background="templates/9.1/images/top_03.jpg"><IMG SRC="templates/9.1/images/top_05.jpg" WIDTH=29 HEIGHT=28></td>
  </tr>
  <tr>
    <td width="16" valign="top" background="templates/9.1/images/top_13.jpg"><IMG SRC="templates/9.1/images/top_13.jpg" WIDTH=16 HEIGHT=21></td>
    <td width="16" valign="bottom" background="templates/9.1/images/top_08.jpg"><img src="templates/9.1/images/clear.gif" width="16" height="1"></td>
    <td width="578" height="93" valign="middle" background="templates/9.1/images/top_08.jpg" class="lawOfficesOf">{{website.firmName}}</td>
    <td width="139" align="center" valign="middle" background="templates/9.1/images/top_08.jpg"><span class="lawOfficesOf">{{website.phone|format_phone}}</span></td>
    <td width="17" valign="top" background="templates/9.1/images/top_08.jpg"><img src="templates/9.1/images/clear.gif" width="17" height="1"></td>
    <td width="12" valign="top" background="templates/9.1/images/top_16.jpg"><IMG SRC="templates/9.1/images/top_16.jpg" WIDTH=12 HEIGHT=10></td>
  </tr>
  <tr>
    <td width="16" rowspan="2" valign="top" background="templates/9.1/images/top_13.jpg">&nbsp;</td>
    <td colspan="4" valign="top" bgcolor="#E0E0E0"><img src="templates/9.1/images/clear.gif" width="1" height="8"></td>
    <td width="12" rowspan="2" valign="top" background="templates/9.1/images/top_16.jpg">&nbsp;</td>
  </tr>
  <tr valign="top">
    <td colspan="4"><table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
          <td width="24%" align="left" valign="top" background="templates/9.1/images/fade_maron_1.jpg"><TABLE WIDTH=195 BORDER=0 CELLPADDING=0 CELLSPACING=0>
            {% for loop_page in website.mainMenu %}
            {# This is the highlighted menu tab #}
            {% if loop_page.url == page.url %}
			  <TR>
                <TD class="menu_bar_on"><table width="100%" cellpadding="1"><tr><td height="15"><a class="menutext_on" href="{{loop_page.url}}">{{loop_page.linkName}}</a></td></tr></table></TD>
              </TR>
              <TR>
                <TD bgcolor="#000000"> <img src="templates/9.1/images/clear.gif" width="1" height="2"></TD>
              </TR>
            {% else %}
            {# These are the regular menu tabs #}
			   <TR>
                <TD class="menu_bar"><table width="100%" cellpadding="1" ><tr><td height="15"><a class="menutext" href="{{loop_page.url}}">{{loop_page.linkName}}</a></td></tr></table></TD>
              </TR>
              <TR>
                <TD bgcolor="#000000"> <img src="templates/9.1/images/clear.gif" width="1" height="2"></TD>
              </TR>
            {% endif %}
            {% endfor %}
          </TABLE></td>
          <td width="76%" valign="top"><img src="templates/9.1/images/main_01.jpg" width="183" height="212"><img src="templates/9.1/images/main_02.jpg" width="189" height="212"><img src="templates/9.1/images/main_03.jpg" width="183" height="212"></td>
        </tr>
      </table></td>
  </tr>
  <tr>
    <td width="16" valign="top" background="templates/9.1/images/top_13.jpg">&nbsp;</td>
    <td colspan="4" valign="top">
<table width="750" height="211" border="0" cellpadding="0" cellspacing="0">
        <tr>
          <td width="10" height="226" rowspan="3">&nbsp;</td>
          <td width="175" rowspan="3" valign="top"></td>
          <td width="10" rowspan="3">&nbsp;</td>
          <td width="15" rowspan="3" valign="top" bgcolor="#E0E0E0"></td>
          <td width="530" height="10" bgcolor="#E0E0E0"></td>
          <td width="10" rowspan="3" valign="top" bgcolor="#E0E0E0">&nbsp;</td>
        </tr>
        <tr>
          <td height="35" valign="top" bgcolor="#E0E0E0"><table width="100%" border="0" cellspacing="0" cellpadding="0">
            <tr>
              <td><span class="lawOfficesOf">{{page.title}}</span></td>
            </tr>
            <tr>
              <td>&nbsp;</td>
            </tr>
            <tr>
              <td class="innertext2a">
                <span class="innertext2b">
                  {{page.body}}
                </span>
              </td>
            </tr>
          </table></td>
        </tr>
        <tr>
          <td height="15" valign="top" bgcolor="#E0E0E0">&nbsp;</td>
        </tr>
        <tr>
          <td height="128" bgcolor="#B30303">&nbsp;</td>
          <td align="center" valign="middle" bgcolor="#B30303">&nbsp;</td>
          <td bgcolor="#B30303">&nbsp;</td>
          <td>&nbsp;</td>
          <td valign="top"><table width="482" border="0" cellspacing="0" cellpadding="0">
              <tr>
                <td width="302" height="34">&nbsp;</td>
                <td width="180" height="50"><font color="#B30303" size="4" face="Times New Roman, Times, serif">Contact us </font></td>
              </tr>
              <tr>
                <td valign="top" class="lefttext">
                  {{website.firmName}}<br />
                  {{website.address}}<br />
                  {{website.city}}  {{website.state}} {{website.zip}}</td>
                <td valign="top" class="lefttext">
                  <span class="bodytext"><span class="innertext2a">
                    Phone: {{website.phone|format_phone}}<br />
                    Fax: {{website.fax|format_phone}}<br />
                    Email: {{website.email}}</span></span>
                    {% if website.socialMedia.avvo  %}
                    <div style="margin:5px 0px;">
                      <a href="{{website.socialMedia.avvo}}" id="avvo_badge" title="View my profile on Avvo"><img src="templates/base/images/avvo_badge.jpg" alt="View my profile on Avvo" title="View my profile on Avvo" /></a>
                    </div>
                    {% endif %}
                </td>
              </tr>
            </table></td>
          <td>&nbsp;</td>
        </tr>
        <tr bgcolor="#DC9291">
          <td height="1" colspan="6"><img src="templates/9.1/images/clear.gif" width="1" height="8"></td>
        </tr>
      </table></td>
    <td width="12" valign="top" background="templates/9.1/images/top_16.jpg">&nbsp;</td>
  </tr>
  <tr >
    <td colspan="2" align="left" valign="bottom" background="templates/9.1/images/botom_back.jpg"><img src="templates/9.1/images/botom_corner.jpg" width="32" height="56"></td>
    <td valign="top" background="templates/9.1/images/botom_back.jpg"><a href="http://esqsites123.com" class="esq_link_text"><br>
    Legal web design made easy - Esqsites123.com</a></td>
    <td width="139" align="right" valign="top" background="templates/9.1/images/botom_back.jpg" class="t01"><table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
          <td height="30" align="right" class="t01"><div align="right">
            {% for loop_page in website.footerMenu %}
              <a href="{{loop_page.url}}" class="innertext2a">{{loop_page.linkName}}</a>
              {% if not loop.last %}
              <font color="black"> &nbsp;|&nbsp; </font>
              {% endif %}
            {% endfor %}
		</div></td>
        </tr>
    </table> </td>
    <td colspan="2" align="right" valign="bottom" background="templates/9.1/images/botom_back.jpg"><img src="templates/9.1/images/botom_corner2.jpg" width="29" height="56"></td>
  </tr>
</table>
</body>
</html>