<!-- 21.1 twig -->
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
  {% block title none|include_title %}
  {{none|include_http_metas}}
  {{none|include_metas}}
  <link href="templates/21.1/css/css.css" rel="stylesheet" type="text/css" />
  <link href="templates/base/css/base.css" rel="stylesheet" type="text/css" />
</head>

<body>
<table width="820" border="0" align="center" cellpadding="0" cellspacing="0" class="txt_01">
  <tr>
    <td height="30" valign="top">
      {% for loop_page in website.mainMenu %}
      <a class="link02" href="{{loop_page.url}}">{{loop_page.linkName}}</a>
      {% if not loop.last %}&nbsp;|&nbsp;{% endif %}
      {% endfor %}
    </td>
  </tr>
  <tr>
    <td height="227" background="templates/21.1/images/flash.jpg"><table width="300" border="0" cellspacing="0" cellpadding="0">
      <tr>
        <td width="50"><img src="templates/21.1/images/esp_cel_inf.gif" width="50" height="1" /></td>
        <td class="txt_05">{{website.firmName|break}}<br /></td>
      </tr>

    </table></td>
  </tr>
  <tr>
    <td bgcolor="#FFFFFF">&nbsp;</td>
  </tr>
  <tr>
    <td bgcolor="#FFFFFF"><table width="761" border="0" align="center" cellpadding="0" cellspacing="0">
      <tr>
        <td width="505"><table width="505" border="0" cellspacing="0" cellpadding="0">
          <tr>
            <td class="txt_08">{{page.title}}</td>
          </tr>
          <tr>
            <td height="20"><img src="templates/21.1/images/img02.jpg" width="505" height="7" /></td>
          </tr>
          <tr>
            <td class="txt_01">
              {{page.body}}
            </td>
          </tr>

        </table></td>
        <td width="50"><img src="templates/21.1/images/esp_cel_inf.gif" width="50" height="1" /></td>
        <td valign="top"><table width="205" border="0" cellspacing="0" cellpadding="0">
          <tr>
            <td height="60" colspan="3" background="templates/21.1/images/img09.jpg"><table width="200" border="0" cellspacing="0" cellpadding="0">
                <tr>
                  <td width="20"><img src="templates/21.1/images/esp_cel_inf.gif" width="20" height="1" /></td>
                  <td class="txt_07">Contact Us </td>
                </tr>
            </table></td>
          </tr>
          <tr>
            <td width="14" background="templates/21.1/images/img10.jpg"><img src="templates/21.1/images/img10.jpg" width="14" height="3" /></td>
            <td width="178" bgcolor="#dae2e8" class="txt_02">{{website.address}}<br />
			{{website.city}}, {{website.state}} {{website.zip}}<br />
              Phone: {{website.phone|format_phone}} <br />
			  Fax: {{website.fax|format_phone}} <br />
			  
              {#
              Email: <a href="mailto:{$email}" class="arial_14_li">{{website.email}}</a>
              #}
              Email: <a href="mailto:{{website.email}}" class="arial_14_li">{{website.email}}</a>
              <div id=badge_container" style="margin-top:4px;">
                {% if website.socialMedia.avvo  %}
                  <a href="{{website.socialMedia.avvo}}" id="avvo_badge" title="View my profile on Avvo"><img src="templates/base/images/avvo_badge.jpg" alt="View my profile on Avvo" title="View my profile on Avvo" /></a>
                {% endif %}
              </div>
            </td>
            <td width="14" align="right" valign="top" background="templates/21.1/images/img11.jpg"><img src="templates/21.1/images/img11.jpg" width="14" height="3" /></td>
          </tr>
          <tr>
            <td colspan="3"><img src="templates/21.1/images/img12.jpg" width="205" height="14" /></td>
          </tr>
        </table></td>
      </tr>

    </table></td>
  </tr>
  <tr>
    <td bgcolor="#FFFFFF">&nbsp;</td>
  </tr>
  <tr>
    <td background="templates/21.1/images/img07.jpg"><img src="templates/21.1/images/img07.jpg" width="4" height="2" /></td>
  </tr>
  <tr>
    <td height="66" align="right" bgcolor="#335e7d"><table width="761" border="0" align="center" cellpadding="0" cellspacing="0">
      <tr>
        <td height="20" class="txt_03"><div align="left">
          {{website.address}}, {{website.city}}, {{website.state}} {{website.zip}} &nbsp;&nbsp;
          Phone: {{website.phone|format_phone}} &nbsp;
          Email: <a href="mailto:{{website.email}}" class="arial_14_li">{{website.email}}</a>
				</div></td>
      </tr>
      <tr>
        <td height="20" class="txt_04">
          <div align="left">
          {% for loop_page in website.mainMenu %}
            <a class="link01" href="{{loop_page.url}}">{{loop_page.linkName}}</a>
            {% if not loop.last %}&nbsp;|&nbsp;{% endif %}
          {% endfor %}
          &nbsp;|&nbsp;
          {% for loop_page in website.footerMenu %}
            <a class="link01" href="{{loop_page.url}}">{{loop_page.linkName}}</a>
            {% if not loop.last %}&nbsp;|&nbsp;{% endif %}
          {% endfor %}
          </div></td>
      </tr>

	      <tr>
        <td height="65"><div align="center" class="txt_03">
          Copyright &copy; 2009. All world rights reserved.  Unauthorized duplication is unlawful.<br />
          Legal web design made easy - EsqSites123.com</div></td>
      </tr>
</table>

    </table></td>
  </tr>
</table>
</body>
</html>