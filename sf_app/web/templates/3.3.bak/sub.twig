<!-- 3.3 twig -->
<html>
<head>
  {% block title none|include_title %}
  {{none|include_http_metas}}
  {{none|include_metas}}
  <link href="templates/3.3/css/css.css" rel="stylesheet" type="text/css">
  <link href="templates/base/css/base.css" rel="stylesheet" type="text/css">
</head>

<body bgcolor="#97A8AF" leftmargin="0" topmargin="0" marginwidth="0" marginheight="0">
<table width="723" height="227" border="0" align="center" cellpadding="0" cellspacing="0" bgcolor="#FFFFFF" class="border">
  <tr>
    <td height="106" colspan="2" align="left"><table width="100%"  border="0" cellspacing="0" cellpadding="0">
      <tr>
        <td width="2%" height="106" background="templates/3.3/images/bg_top.jpg">&nbsp;</td>
        <td width="98%" background="templates/3.3/images/bg_top.jpg"><span class="firmName">{{website.firmName}}</span></td>
      </tr>
    </table></td>
  </tr>
   <tr>
<td colspan="3" background="templates/3.3/images/menu_bg.jpg">
  <table width="" border="0" cellpadding="8" cellspacing="0">
    {% for loop_page in website.mainMenu %}
    {# This is the highlighted menu tab #}
	{% if loop_page.url == page.url %}
      <td background="templates/3.3/images/menu_bg_rollover.jpg" align="center" valign="bottom" height="48">
        <strong><a class="menutext" href="{{loop_page.url}}">{{loop_page.linkName}}</a></strong>
      </td>
    {% else %}
	{# These are the regular menu tabs #}
      <td  align="center" valign="bottom" height="48">
      <strong><a class="menutext" href="{{loop_page.url}}">{{loop_page.linkName}}</a></strong>
      </td>
    {% endif %}
    {% endfor %}
  </table>
</td>
</tr>
   <tr>
     <td height="19" colspan="2" valign="top"><table width="100%"  border="0" cellspacing="0" cellpadding="0">
       <tr>
         <td width="30%" valign="top"><table width="93%"  border="0" cellspacing="0" cellpadding="0">
           <tr>
             <td><img src="templates/3.3/images/mallet.jpg" width="201" height="191"></td>
           </tr>
           <tr>
             <td><table width="100%"  border="0" cellspacing="0" cellpadding="0">
               <tr>
                 <td width="6%">&nbsp;</td>
                 <td width="89%">
                   <p><font color="#A13F14" size="2" face="Arial, Helvetica, sans-serif"><strong><br />
                   Contact us<br />
                     </strong></font><br />
                     <span class="innertext2a">
                       Phone: {{website.phone|format_phone}}<br />
                       Fax: {{website.fax|format_phone}}<br />
                       Email: {{website.email}}
                     </span>
                   </p>
                   <p class="innertext2a">
                     {{website.address}}<br />
                     {{website.city}} {{website.state}} {{website.zip}}
                   </p>
                   {% if website.socialMedia.avvo  %}
                    <div style="margin:5px 0px;">
                      <a href="{{website.socialMedia.avvo}}" id="avvo_badge" title="View my profile on Avvo"><img src="templates/base/images/avvo_badge.jpg" alt="View my profile on Avvo" title="View my profile on Avvo" /></a>
                    </div>
                    {% endif %}
                 </td>
                 <td width="5%">&nbsp;</td>
               </tr>
             </table>               <p>&nbsp;</p>
               </td>
           </tr>
         </table></td>
         <td width="70%" valign="top"><table width="100%"  border="0" cellspacing="0" cellpadding="0">
           <tr>
             <td>&nbsp;</td>
           </tr>
           <tr>
             <td><span class="slogan">{{page.title}}</span></td>
           </tr>
           <tr>
             <td>&nbsp;</td>
           </tr>
           <tr>
             <td class="innertext2a"><table width="100%"  border="0" cellspacing="0" cellpadding="0">
               <tr>
                 <td width="96%" class="innertext2a">
                    {{page.body}}
                 </td>
                 <td width="4%">&nbsp;</td>
               </tr>
             </table></td>
           </tr>
         </table></td>
       </tr>
     </table></td>
   </tr>
   <tr>
     <td width="494" height="19" valign="top">&nbsp;</td>
     <td width="227" height="19" align="right" valign="top">&nbsp;</td>
   </tr>
  <tr>
    <td height="54" colspan="2" align="right" class="t01"><table width="100%"  border="0" cellspacing="0" cellpadding="0">
      <tr>
        <td height="10" colspan="4"></td>
        </tr>
      <tr>
        <td width="4%">&nbsp;</td>
        <td width="55%" class="t01">{{website.firmName}}</td>
        <td width="38%"><div align="right">
          {% for loop_page in website.footerMenu %}
            <a href="{{loop_page.url}}" class="innertext2">{{loop_page.linkName}}</a>
            {% if not loop.last %}
            <font color="grey"> &nbsp;|&nbsp; </font>
            {% endif %}
		  {% endfor %}
		</div></td>
        <td width="3%">&nbsp;</td>
      </tr>
      <tr>
        <td height="10"></td>
        <td height="10" class="esq_link_text"><a href="http://esqsites123.com" class="esq_link_text">Legal web design</a> by Esqsites123.com</td>
        <td height="10" colspan="2"></td>
      </tr>
    </table></td>
  </tr>
</table>
</body>
</html>