<!-- 6.1 twig -->
<html>
<head>
  {% block title none|include_title %}
  {{none|include_http_metas}}
  {{none|include_metas}}
  <link href="templates/6.1/css/css.css" rel="stylesheet" type="text/css">
  <link href="templates/base/css/base.css" rel="stylesheet" type="text/css">
</head>

<body leftmargin="0" topmargin="0" marginwidth="0" marginheight="0">
<table width="100%" border="0" cellspacing="0" cellpadding="0">

  <tr>
    <td height="68" align="left" valign="bottom" bgcolor="#A47142"><TABLE WIDTH=748 BORDER=0 CELLPADDING=0 CELLSPACING=0 bgcolor="#FFF7B8">

        <TR>
          <td colspan="3" align="center" background="templates/6.1/images/menu_bg.jpg">

<table height="70" border="0" cellpadding="8" cellspacing="0" >
	<tr>
	 {% for loop_page in website.mainMenu %}
     {# This is the highlighted menu tab #}
     {% if loop_page.url == page.url %}
        <td background="templates/6.1/images/menu_bg_rollover.jpg" align="center" valign="bottom" height="35">
          <strong><a class="menutext_on" href="{{loop_page.url}}">{{loop_page.linkName}}</a></strong>
        </td>
      {% else %}
      {# These are the regular menu tabs #}
        <td  align="center" valign="bottom" height="35">
          <strong><a class="menutext" href="{{loop_page.url}}">{{loop_page.linkName}}</a></strong>
		</td>
      {% endif %}
      {% endfor %}
	</tr>
</table>
</td>
        </TR>
      </TABLE></td>
  </tr>
  <tr>
    <td height="227" align="center" valign="bottom" background="templates/6.1/images/main_06.jpg">
<table width="748" border="0" cellspacing="0" cellpadding="0">
        <tr>
          <td width="268" height="227" align="left" valign="middle" background="templates/6.1/images/main_02.jpg"><table width="100%"  border="0" cellspacing="0" cellpadding="0">
            <tr>
              <td width="8%">&nbsp;</td>
              <td width="83%" class="lawOfficesOf_white">{{website.firmName|break}}</td>
              <td width="9%">&nbsp;</td>
            </tr>
            <tr>
              <td>&nbsp;</td>
              <td>&nbsp;</td>
              <td>&nbsp;</td>
            </tr>
          </table></td>
          <td width="480"><img src="templates/6.1/images/main_03.jpg"></td>
        </tr>
      </table>
    </td>
  </tr>
  <tr>
    <td valign="top" align="center" bgcolor="#A47142"><table width="748" border="0" cellpadding="0" cellspacing="0" bgcolor="#FFF7B8">
        <tr>
          <td width="24" height="100">&nbsp;</td>
          <td colspan="2" valign="top"><table width="100%" border="0" cellspacing="0" cellpadding="0">
              <tr>
                <td >&nbsp;</td>
              </tr>
              <tr>
                <td ><span class="lawOfficesOf">{{page.title}}</span></td>
              </tr>
              <tr>
                <td >&nbsp;</td>
              </tr>
              <tr>
                <td width="690" class="innertext2">
                  {{page.body}}
                </td>
              </tr>
          </table></td>
          <td width="24">&nbsp;</td>
        </tr>
        <tr>
          <td>&nbsp;</td>
          <td height="35" colspan="2" align="center" valign="top">...............................................................................................................................................................................</td>
          <td>&nbsp;</td>
        </tr>
        <tr>
          <td>&nbsp;</td>
          <td width="225" valign="top"> </td>
          <td width="475" valign="top" ><table width="100%" border="0" cellspacing="0" cellpadding="0">
              <tr>
                <td width="4%" height="74" rowspan="2" valign="top" class="innertext2">&nbsp;</td>
                <td width="50%" rowspan="2" valign="top" class="innertext2">&nbsp;</td>
                <td width="3%" rowspan="2" valign="top">&nbsp;</td>
                <td width="43%" height="20" valign="top" class="innertext2">
                  <p><font size="3" face="Arial, Helvetica, sans-serif"><strong>Contact us </strong></font></p></td>
              </tr>
              <tr valign="top">
                <td class="innertext2">
                  <span class="innertext2a">
                    Phone: {{website.phone|format_phone}}<br />
                    Fax: {{website.fax|format_phone}}<br />
                    Email: <a href="mailto:{{website.email}}">{{website.email}}</a></span>
                    {% if website.socialMedia.avvo  %}
                    <div style="margin:5px 0px;">
                      <a href="{{website.socialMedia.avvo}}" id="avvo_badge" title="View my profile on Avvo"><img src="templates/base/images/avvo_badge.jpg" alt="View my profile on Avvo" title="View my profile on Avvo" /></a>
                    </div>
                    {% endif %}
                </td>
              </tr>
            </table></td>
          <td>&nbsp;</td>
        </tr>
        <tr>
          <td>&nbsp;</td>
          <td>&nbsp;</td>
          <td>&nbsp;</td>
          <td>&nbsp;</td>
        </tr>
        <tr>
          <td><img src="templates/6.1/images/clear.gif" width="1" height="2"></td>
          <td colspan="2" bgcolor="#000000"><img src="templates/6.1/images/clear.gif" width="1" height="2"></td>
          <td><img src="templates/6.1/images/clear.gif" width="1" height="2"></td>
        </tr>
        <tr>
          <td>&nbsp;</td>
          <td class="innertext2">
            {{website.address}}<br />
            {{website.city}} {{website.state}} {{website.zip}}
          </td>
          <td height="50" align="right" class="t01"><div align="right">
            {% for loop_page in website.footerMenu %}
              <a href="{{loop_page.url}}" class="menutext">{{loop_page.linkName}}</a>
              {% if not loop.last %}
              <font color="black"> &nbsp;|&nbsp; </font>
              {% endif %}
            {% endfor %}
		</div></td>
          <td>&nbsp;</td>
        </tr>
        <tr>
          <td height="25">&nbsp;</td>
          <td height="25" class="innertext2">&nbsp;</td>
          <td height="25" align="right" class="innertext2"><a href="http://esqsites123.com/">Legal Web Design</a> by EsqSites123.com </td>
          <td height="25">&nbsp;</td>
        </tr>
      </table></td>
  </tr>
</table>
</body>
</html>