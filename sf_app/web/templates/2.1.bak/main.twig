<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  {% block title none|include_title %}
  {{none|include_http_metas}}
  {{none|include_metas}}
  <link href="templates/2.1/css/css.css" rel="stylesheet" type="text/css">
  <link href="templates/base/css/base.css" rel="stylesheet" type="text/css">
</head>

<body>

	<!--header start-->
    
    <div id="header">
    
    	<div id="headerarea">
        
        	<!--logo start-->
        
        	<!--logo end-->
        
        	<!--contactno start-->
        
        	<!--contactno end-->
        
        </div>
    
    </div>

	<!--header start-->

	<!--navigation start-->
    
    <div id="nav">
    
   		<div id="navarea">
        
        	<ul>
                {% for loop_page in website.mainMenu %}                
                    <li>
                        <a href="{{loop_page.url}}" {% if loop_page.url == page.url %}class="navactive"{% endif %}>{{ loop_page.linkName }}</a>
                    </li>
                {% endfor %}            
            </ul>
        
        </div>
    
    </div>

	<!--navigation end-->

	<!--banner start-->
    
    <div class="banlrcon">
    
    	<div class="banleft">
        
        	{{website.firmName|break}}
        
        </div>
    
    	<div id="banner">
        <img src="templates/2.1/images/bannerimg01.png" alt="template" title="template" />
        </div>
    
    </div>

	<!--banner end-->

	<!--body start-->
    
    <div id="conarea">
    
    	<!--bodyleft start-->
        
        <div id="conarealeft">
        
        	<div class="conleft">
            
                <h2>
                Contact Us
                </h2>
                
                
                {{website.address}}<br />
                {{website.city}}<br />
                {{webiste.state}} {{website.zip}}<br /><br />
                
                
                Phone: {{website.phone|format_phone}}<br />
                Fax: {{website.fax|format_phone}}<br />
                Email: <a href="mailto:{{website.email}}">{{website.email}}</a>
                
            
            </div>
        
        </div>
    
    	<!--bodyleft end-->
    
    	<!--bodyright start-->
        
        <div id="conarearight">
        
        	<div class="conright">
            
            	<h3>
                {{page.title}}
                </h3>
                
                <div class="conboxright">
                {{page.content}}
                </div>
            
            </div>
        
        </div>
    
    	<!--bodyright end-->
    
    </div>

	<!--body end-->

	<!--footer start-->
    
    <div id="footer">
    
    	<!--footerbox start-->
    
    	<div id="footercon">
        
        	<div id="footerconarea">
            
            	<!--conleft start-->
                
                <div id="footerconleft">
                
                	<ul>
                    
                    	<li><img src="templates/2.1/images/icon1.png" alt="icon" /></li>
                    
                    	<li>Legal Web Design by esqsites123.com</li>
                    
                    </ul>
                
                </div>
            
            	<!--conleft end-->
            
            	<!--conright start-->
                
                <div id="footerconright">
                
                	<ul>
                        {% for loop_page in website.footerMenu %}
                            <li>
                                <a href="{{loop_page.url}}">{{loop_page.linkName}}</a>
                            </li>
                        {% endfor %}
                    </ul>
                
                </div>
            
            	<!--conright end-->
            
            </div>
        
        </div>
    
    	<!--footerbox end-->
    
    	<!--footertext start-->
        
        <div id="footercopy">
        Copyright&copy; 2012. All world rights reserved
        </div>
    
    	<!--footertext end-->
    
    </div>

	<!--footer end-->

</body>
</html>
